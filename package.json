{
  "name": "ultron",
  "version": "0.2.0",
  "description": "",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --quiet",
    "md": "ghmd --title 更新日志 --dest app/changelog.html CHANGELOG.md",
    "clean": "rm -rf ./dist",
    "clean:osx": "rm -rf ./dist/osx",
    "clean:win": "rm -rf ./dist/win",
    "build": "npm run clean && npm run build:vue && npm run build:osx && npm run build:win",
    "build:vue": "NODE_ENV=production webpack --progress --hide-modules",
    "build:osx": "npm run clean:osx && electron-packager ./app \"Ultron\" --out=dist/osx --platform=darwin --arch=x64 --version=0.36.1",
    "build:win": "npm run clean:win && electron-packager ./app \"Ultron\" --out=dist/win --platform=win32 --arch=ia32 --version=0.36.1",
    "pack": "npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run build:osx && electron-builder \"dist/osx/Ultron-darwin-x64/Ultron.app\" --platform=osx --out=\"dist/osx\" --config=packager.json",
    "pack:win": "npm run build:win && electron-builder \"dist/win/Ultron-win32-ia32\" --platform=win --out=\"dist/win\" --config=packager.json"
  },
  "homepage": "https://github.com/17173/ultron",
  "bugs": "https://github.com/17173/ultron/issues",
  "author": "lynzz",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babel-runtime": "^5.8.0",
    "css-loader": "^0.23.0",
    "electron-packager": "^5.2.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "function-bind": "^1.0.2",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.0",
    "svg-inline-loader": "^0.4.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "vue": "^1.0.12",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^7.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.0",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-target-electron-renderer": "^0.3.0"
  }
}
